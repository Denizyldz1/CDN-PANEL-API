@model List<FileModel>
@{
    ViewData["Title"] = "Index";

}

<div class="card">
    <div class="card-body">
        <h4 class="card-title">Link Bilgileri</h4>
        <div class="table-responsive pt-3">
            <table class="table table-bordered">
                <thead>
                    <tr>
                        <th>*</th>
                        <th>FileName</th>
                        <th>FileType</th>
                        <th>FileSize</th>
                        <th>FilePatchUrl</th>
                        <th>UserId</th>
                        <th>UserName</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>@item.Id</td>
                            <td>@item.FileName</td>
                            <td>@item.FileType</td>
                            <td>@item.FileSize</td>
                            <td>@item.FilePatchUrl</td>
                            <td>@item.UserId</td>
                            <td>@item.UserName</td>
                        </tr>

                    }
                </tbody>
            </table>
        </div>
    </div>
</div>
